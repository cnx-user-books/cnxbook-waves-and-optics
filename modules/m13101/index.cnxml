                                 <m:mi>R</m:mi>
                               </m:mrow>
                             </m:mrow>
                             <m:mo stretchy="false" fence="true" form="postfix">)</m:mo>
                           </m:mrow>
                         </m:mrow>
                       </m:msup>
                     </m:mrow>
                     <m:mi>R</m:mi>
                   </m:mfrac>
                   <m:mo/>
                   <m:mrow>
                     <m:msubsup>
                       <m:mo form="prefix" largeop="true">∫</m:mo>
                       <m:mtext/>
                       <m:mtext/>
                     </m:msubsup>
                     <m:mrow>
                       <m:mrow>
                         <m:msubsup>
                           <m:mo form="prefix" largeop="true">∫</m:mo>
                           <m:mrow>
                             <m:mi>A</m:mi>
                             <m:mo/>
                             <m:mi>p</m:mi>
                             <m:mo/>
                             <m:mi>e</m:mi>
                             <m:mo/>
                             <m:mi>r</m:mi>
                             <m:mo/>
                             <m:mi>t</m:mi>
                             <m:mo/>
                             <m:mi>u</m:mi>
                             <m:mo/>
                             <m:mi>r</m:mi>
                             <m:mo/>
