                             <m:mo/>
                             <m:mi>p</m:mi>
                             <m:mo/>
                             <m:mi>e</m:mi>
                             <m:mo/>
                             <m:mi>r</m:mi>
                             <m:mo/>
                             <m:mi>t</m:mi>
                             <m:mo/>
                             <m:mi>u</m:mi>
                             <m:mo/>
                             <m:mi>r</m:mi>
                             <m:mo/>
                             <m:mi>e</m:mi>
                           </m:mrow>
                           <m:mtext/>
                         </m:msubsup>
                         <m:mrow>
                           <m:msup>
                             <m:mi>e</m:mi>
                             <m:mrow>
                               <m:mi>i</m:mi>
                               <m:mo/>
                               <m:mi>y</m:mi>
                               <m:mo/>
