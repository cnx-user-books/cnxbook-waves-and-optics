                           <m:mi>y</m:mi>
                         </m:mrow>
                       </m:mfrac>
                       <m:msub>
                         <m:mi>E</m:mi>
                         <m:mi>y</m:mi>
                       </m:msub>
                     </m:mrow>
                     <m:mo/>
                     <m:msup>
                       <m:mi>e</m:mi>
                       <m:mrow>
