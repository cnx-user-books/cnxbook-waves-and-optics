                       <m:mi>e</m:mi>
                       <m:mrow>
                         <m:mi>i</m:mi>
                         <m:mo/>
                         <m:mrow>
                           <m:mover accent="true">
                             <m:mi>k</m:mi>
                             <m:mo accent="true" form="postfix">⃗</m:mo>
                           </m:mover>
                           <m:mo form="infix">⋅</m:mo>
                           <m:mover accent="true">
