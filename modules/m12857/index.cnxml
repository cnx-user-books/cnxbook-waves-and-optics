                         <m:mover accent="true">
                           <m:mi>r</m:mi>
                           <m:mo accent="true" form="postfix">⃗</m:mo>
                         </m:mover>
                       </m:mrow>
                     </m:mrow>
                   </m:msup>
                   <m:mo/>
                   <m:mrow>
                     <m:mfrac>
                       <m:mo form="prefix">∂</m:mo>
                       <m:mrow>
                         <m:mo form="prefix">∂</m:mo>
                         <m:mi>x</m:mi>
                       </m:mrow>
                     </m:mfrac>
                     <m:mrow>
