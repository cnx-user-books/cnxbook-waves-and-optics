                         <m:mo/>
                         <m:mrow>
                           <m:mover accent="true">
                             <m:mi>k</m:mi>
                             <m:mo accent="true" form="postfix">⃗</m:mo>
                           </m:mover>
                           <m:mo form="infix">⋅</m:mo>
                           <m:mover accent="true">
                             <m:mi>r</m:mi>
                             <m:mo accent="true" form="postfix">⃗</m:mo>
                           </m:mover>
                         </m:mrow>
                       </m:mrow>
                     </m:msup>
                   </m:mrow>
                   <m:mo form="infix">+</m:mo>
                   <m:mrow>
                     <m:mrow>
                       <m:mfrac>
                         <m:mo form="prefix">∂</m:mo>
