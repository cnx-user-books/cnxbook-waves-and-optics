                       </m:mrow>
                     </m:mrow>
                   </m:msup>
                 </m:mrow>
                 <m:maligngroup/>
                 <m:mo form="infix">=</m:mo>
                 <m:maligngroup/>
                 <m:maligngroup/>
                 <m:mrow>
                   <m:mrow>
                     <m:mrow>
                       <m:mfrac>
                         <m:mo form="prefix">∂</m:mo>
                         <m:mrow>
                           <m:mo form="prefix">∂</m:mo>
                           <m:mi>x</m:mi>
                         </m:mrow>
                       </m:mfrac>
                       <m:msub>
                         <m:mi>E</m:mi>
                         <m:mi>x</m:mi>
                       </m:msub>
                     </m:mrow>
                     <m:mo/>
